{
    "manifest_version": 2,
    "name": "kenBuddy",
    "description": "__MSG_extensionDescription__",
    "version": "0.1.4",
    "icons": {
        "16": "logo/logo-16.png",
        "48": "logo/logo-48.png",
        "128": "logo/logo-128.png"
    },
    "options_ui": {
        "page": "settings/settings.html",
        "open_in_tab": true
    },
    "permissions": ["storage","https://api.kenjo.io/*"],
    "content_scripts": [{
        "js": ["firefox/utils.js","utils/utils.js","kenbuddy.js"],
        "matches": ["https://app.kenjo.io/*"]
    }],
    "default_locale": "en"
}
